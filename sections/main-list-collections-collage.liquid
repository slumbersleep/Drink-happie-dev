{%- liquid
  assign items = collections
  assign block_collections = section.blocks | where: 'type', 'collection'
  if block_collections.size > 0
    assign using_blocks = true
    assign items = block_collections
  endif
-%}
{%- paginate collections by 6 -%}
  <section
    class="relative bg-scheme-background py-section-vertical-spacing text-scheme-text {% if section.settings.use_gradient_background %}bg-scheme-gradient{% endif %} overflow-hidden"
    data-color-scheme="{{ section.settings.color_scheme }}"
  >
    <div class="mx-auto grid max-w-site px-section lg:grid-flow-row-dense lg:auto-rows-auto lg:grid-cols-12 {% if section.settings.title != '' %}section-content-top-margin{% endif %} gap-theme">
      {% for item in items %}
        {%- liquid
          if section.index == 1 and forloop.first
            assign preload = true
          else
            assign preload = false
          endif

          if section.index <= 3 and forloop.index <= 3
            assign loading = 'eager'
          else
            assign loading = 'lazy'
          endif

          assign default_label = 'homepage.onboarding.collection_title' | t
          assign collection = item
          assign image_shape = section.settings.image_shape

          if using_blocks
            assign collection = item.settings.collection
            if item.settings.image_shape != 'same_as_section'
              assign image_shape = item.settings.image_shape
            endif
            assign rotate_image = item.settings.rotate_image
          endif

          assign url = collection.url
          assign image = collection.image | default: collection.products.first.featured_image
          assign button_text = collection.title | default: default_label

          capture desktop_size
            cycle 'lg:col-span-8 lg:row-span-2', 'lg:col-span-4', 'lg:col-span-4', 'lg:col-span-4', 'lg:col-span-4 lg:row-start-4', 'lg:col-span-8 lg:row-span-2'
          endcapture

          assign large_item_sizes = 'calc(100vw / 12 * 8)'
          assign item_sizes = 'calc(100vw / 12 * 4)'

          capture desktop_sizes
            cycle large_item_sizes, item_sizes, item_sizes, item_sizes, item_sizes, large_item_sizes
          endcapture

          assign sizes = '(min-width: 990px) ' | append: desktop_sizes | append: ', ' | append: '100vw'

          assign shape_id = section.id | append: '-' | append: item.id

          if image_shape == 'blob'
            assign shape_id = section.id | append: '-' | append: item.id | append: '-' | append: forloop.index0
          endif
        -%}

        <div
          class="gallery-item relative flex items-center justify-center {{ desktop_size }}"
          {% if using_blocks %}
            {{ item.shopify_attributes }}
          {% endif %}
        >
          <a href="{{ url }}" class="group block w-full">
            {%- render 'shape-defs',
              shape: image_shape,
              shape_id: shape_id,
              index: forloop.index0
            -%}

            {%- if image != blank -%}
              <div class="w-full">
                {%- render 'media-image',
                  media: image,
                  shape: image_shape,
                  shape_id: shape_id,
                  crop: section.settings.crop,
                  is_card: false,
                  rotate: rotate_image,
                  container_id: section.id,
                  index: forloop.index0,
                  preload: preload,
                  loading: loading,
                  sizes: sizes
                -%}
              </div>
            {%- else -%}
              <div class="w-full">
                {%- render 'media-image-placeholder',
                  shape_id: shape_id,
                  container_id: section.id,
                  shape: image_shape,
                  crop: section.settings.crop,
                  is_card: false,
                  index: forloop.index0
                -%}
              </div>
            {%- endif -%}

            {%- if button_text != blank -%}
              <div class="{% if section.settings.show_buttons_on_mobile %}flex{% else %}hidden lg:flex{% endif %} gallery-item__content absolute bottom-0 left-0 right-0 top-0  z-10 items-center justify-center px-5 text-center text-scheme-text  {% if section.settings.only_show_button_on_hover %}lg:opacity-0 group-hover:lg:opacity-100 transition duration-200 ease-in-out{% endif %}">
                <span class="push-btn  inline-block break-words text-center">
                  <span class="{{ section.settings.button_style }}">
                    {{- button_text -}}
                  </span>
                </span>
              </div>
            {%- endif -%}
          </a>
        </div>
      {%- endfor -%}
    </div>

    {%- unless using_blocks -%}
      {%- if paginate.pages > 1 -%}
        <div class="mx-auto max-w-site px-section pt-section-vertical-spacing">
          {%- render 'pagination', paginate: paginate, anchor: '' -%}
        </div>
      {%- endif -%}
    {%- endunless %}

    {%- liquid
      for block in section.blocks
        case block.type
          when 'background_shape'
            render 'block-background-shape', block: block
          when 'background_pattern'
            render 'block-background-pattern', block: block, scheme: section.settings.color_scheme
          when 'shape_divider'
            render 'block-shape-divider', block: block
        endcase
      endfor
    -%}
  </section>
{%- endpaginate -%}

{%- assign stickers = section.blocks | where: 'type', 'sticker' -%}
{%- liquid
  for sticker in stickers
    render 'block-sticker', block: sticker
  endfor
-%}

