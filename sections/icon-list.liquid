{%- liquid
  assign block_heading_level = 'h2'
  assign icons = section.blocks | where: 'type', 'icon'
  assign icon_max_width = section.settings.max_width
  assign layout_on_mobile = section.settings.layout_on_mobile

  assign container_mobile_classes = ''
  if layout_on_mobile == 'wrap'
    assign list_mobile_classes = 'grid'
    assign divisible_by_three = icons.size | modulo: 3
    if divisible_by_three == 0
      assign list_mobile_classes = list_mobile_classes | append: ' grid-cols-3'
    else
      assign list_mobile_classes = list_mobile_classes | append: ' md:grid-cols-2'
    endif
  else
    assign container_mobile_classes = '-mx-section'
    assign list_mobile_classes = 'flex px-section overflow-x-auto snap-x snap-promixity'
    assign item_mobile_classes = 'scroll-mx-section snap-start shrink-0 w-[var(--icon-max-width)]'
  endif

  assign icon_align = 'items-center'
  for icon in icons
    if icon.settings.text != ''
      assign icon_align = 'items-baseline'
      break
    endif
  endfor
-%}

<style>
  #icon-row-{{ section.id }} {
    --icon-max-width: {{ icon_max_width  }}px;
  }
  @media (min-width: 768px) {
    #icon-row-{{ section.id }} {
      --icon-max-width: {{ icon_max_width | times: 1.5}}px;
    }
  }
</style>

<section
  id="section-{{ section.id }}"
  class="relative bg-scheme-background text-scheme-text {% if section.settings.use_gradient_background %}bg-scheme-gradient{% endif %} relative overflow-hidden"
  data-color-scheme="{{ section.settings.color_scheme }}"
>
  <div class="section-content relative z-10 mx-auto max-w-site px-section pb-section-vertical-spacing">
    {% if section.settings.title != '' %}
      {%- assign block_heading_level = 'h3' -%}
      <h2 class="inline-heading-rte font-heading heading-standard  heading-style break-words text-center {{ settings.heading_drop_shadow_style }}">
        {{ section.settings.title }}
      </h2>
    {% endif %}

    <div class="{{ container_mobile_classes }} {% if section.settings.title != '' %}section-content-top-margin{% endif %}">
      <ul
        id="icon-row-{{ section.id }}"
        class="feature-icon-list items-top {{ list_mobile_classes }} {{ icon_align }} mt-8 mb-8 grid-cols-2 md:mt-0 md:flex md:flex-nowrap {{ section.settings.icon_alignment_on_desktop }}"
      >
        {% for block in icons %}
          <li
            class="text-center {{ item_mobile_classes }} space-y-4 md:w-auto md:min-w-0 md:max-w-[var(--icon-max-width)] md:flex-1"
            {{ block.shopify_attributes }}
          >
            <div class="mx-auto">
              <span
                class="mx-auto block text-scheme-text"
                style="max-width: {{ icon_max_width }}px; --icon-thickness: {{ section.settings.icon_thickness }};"
              >
                {%- unless block.settings.custom_image -%}
                  <div class="rounded-full {% if section.settings.show_border %}border-[calc(var(--icon-thickness)_*_1px)]{% else %}border-0{% endif %} block rounded-full border-current {{  section.settings.background_color }}">
                    {%- render 'theme-icon', icon: block.settings.icon -%}
                  </div>
                {%- else -%}
                  {% liquid
                    # theme-check-disable VariableName
                    assign icon_max_width_2x = icon_max_width | times: 2
                    assign icon_max_width_3x = icon_max_width | times: 3
                    # theme-check-enable VariableName

                    assign widths = icon_max_width
                    assign widths = widths | append: ', ' | append: icon_max_width_2x
                    assign widths = widths | append: ', ' | append: icon_max_width_3x

                    assign sizes = icon_max_width | append: 'px'
                  %}
                  {{
                    block.settings.custom_image
                    | image_url: width: icon_max_width_3x
                    | image_tag: widths: widths, sizes: sizes
                  }}
                {%- endunless -%}
              </span>
            </div>
            {% if block.settings.text != '' %}
              <{{ block_heading_level }} class="break-words text-base {% if section.settings.text_style == 'heading' %}font-heading heading-secondary{% endif %} {% if section.settings.uppercase_text %}uppercase{%  endif %}">
                {{- block.settings.text -}}
              </{{ block_heading_level }}>
            {% endif %}
          </li>
        {% endfor %}
      </ul>
    </div>
  </div>

  {%- liquid
    for block in section.blocks
      case block.type
        when 'background_shape'
          render 'block-background-shape', block: block
        when 'background_pattern'
          render 'block-background-pattern', block: block, scheme: section.settings.color_scheme
      endcase
    endfor
  -%}
</section>

{%- liquid
  assign stickers = section.blocks | where: 'type', 'sticker'
  assign shape_dividers = section.blocks | where: 'type', 'shape_divider'
  for sticker in stickers
    render 'block-sticker', block: sticker
  endfor
  for shape_divider in shape_dividers
    render 'block-shape-divider', block: shape_divider
  endfor
-%}

